            <Label layoutX="407.0" layoutY="229.0" text="Фамилия" />
            <Label layoutX="407.0" layoutY="268.0" text="Должность" />
            <TextArea fx:id="resultLabel" prefHeight="486.0" prefWidth="397.0" />
            <TextArea fx:id="logger" layoutX="407.0" layoutY="298.0" prefHeight="188.0" prefWidth="221.0" />
            <TextField fx:id="textFieldDelete" layoutX="514.0" layoutY="46.0" prefHeight="25.0" prefWidth="113.0" />
            <Button layoutX="542.0" layoutY="113.0" mnemonicParsing="false" onAction="#loadTestData" prefHeight="25.0" prefWidth="85.0" text="Тест" />
            <Button fx:id="createUniversityButton" layoutX="406.0" layoutY="79.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" text="Создать ун." onAction="#createUniversity"/>
            <TextField fx:id="universityTextField" layoutX="515.0" layoutY="79.0" prefHeight="25.0" prefWidth="113.0" />
         </children>
      </Pane>
   </children>
</VBox>
